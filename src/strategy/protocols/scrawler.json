{
  "tokemak": {
    "url": "https://www.tokemak.xyz/",
    "preAction": [
      {
        "type": "click",
        "element": "button.enterreactor",
        "description": "click 'Enter the Tokemak'"
      }
    ],
    "action": {
      "root": ".mb-20",
      "filter": ".mb-6",
      "container": "div > div",
      "pool": "div > div:nth-child(1) > div > div > div:nth-child(1)",
      "apy": [
        {
          "type": "deposit",
          "element": "div > div:nth-child(3) > div:nth-child(1) > div:nth-child(1)"
        },
        {
          "type": "vote",
          "element": "div > div:nth-child(3) > div:nth-child(3) > div:nth-child(1)"
        }
      ],
      "tvl": "div > div:nth-child(3) > div:nth-child(2) > div:nth-child(1) > p:nth-child(1)"
    }
  }
}
